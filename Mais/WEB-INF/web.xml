<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
                      http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
  version="4.0"
  metadata-complete="true">

  <servlet>
    <servlet-name>Index</servlet-name>
    <servlet-class>controllers.Index_controller</servlet-class>
  </servlet>
  <servlet-mapping>
      <servlet-name>Index</servlet-name>
      <url-pattern>/index/data</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>Terrain</servlet-name>
    <servlet-class>controllers.Terrain_controller</servlet-class>
  </servlet>
  <servlet-mapping>
      <servlet-name>Terrain</servlet-name>
      <url-pattern>/Terrain/data</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>Resultat</servlet-name>
    <servlet-class>controllers.Resultat_controller</servlet-class>
  </servlet>
  <servlet-mapping>
      <servlet-name>Resultat</servlet-name>
      <url-pattern>/Terrain/result</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>Parcelle</servlet-name>
    <servlet-class>controllers.Annomalie_controller</servlet-class>
  </servlet>
  <servlet-mapping>
      <servlet-name>Parcelle</servlet-name>
      <url-pattern>/Parcelle/data</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>Prevision</servlet-name>
    <servlet-class>controllers.Prevision_controller</servlet-class>
  </servlet>
  <servlet-mapping>
      <servlet-name>Prevision</servlet-name>
      <url-pattern>/Prevision</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>Composition</servlet-name>
    <servlet-class>controllers.Composition_controller</servlet-class>
  </servlet>
  <servlet-mapping>
      <servlet-name>Composition</servlet-name>
      <url-pattern>/Composition</url-pattern>
  </servlet-mapping>


</web-app>
